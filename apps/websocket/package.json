{
  "name": "@azuki/websocket",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json src/server.ts",
    "build": "tsc && tsc-alias --resolve-full-paths",
    "build:native": "cd native && node-gyp rebuild",
    "build:all": "yarn build:native && yarn build",
    "start": "node dist/server.js",
    "test:engine": "tsx scripts/test-engine.ts"
  },
  "dependencies": {
    "@tcg/backend-core": "*",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.49.0",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "node-gyp": "^10.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}

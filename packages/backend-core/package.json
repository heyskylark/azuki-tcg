{
  "name": "@tcg/backend-core",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./database": {
      "types": "./dist/database/index.d.ts",
      "import": "./dist/database/index.js"
    },
    "./schemas": {
      "types": "./dist/drizzle/schemas/index.d.ts",
      "import": "./dist/drizzle/schemas/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./types/auth": {
      "types": "./dist/types/auth.d.ts",
      "import": "./dist/types/auth.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    },
    "./constants/auth": {
      "types": "./dist/constants/auth.d.ts",
      "import": "./dist/constants/auth.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "import": "./dist/errors/index.js"
    },
    "./services/userService": {
      "types": "./dist/services/userService.d.ts",
      "import": "./dist/services/userService.js"
    },
    "./services/authService": {
      "types": "./dist/services/authService.d.ts",
      "import": "./dist/services/authService.js"
    },
    "./services/roomService": {
      "types": "./dist/services/roomService.d.ts",
      "import": "./dist/services/roomService.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.38.0",
    "jose": "^6.1.3",
    "pg": "^8.13.0",
    "uuidv7": "^1.0.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
